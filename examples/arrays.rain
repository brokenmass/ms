const ALPHABET_LENGTH=26
let array = malloc(ALPHABET_LENGTH)

let i = 0


while (lt(i, ALPHABET_LENGTH)) {
  let pos = add(array, i)

  writeByte(pos, add('a', i))

  print(readByte(pos))

  i = add(i, 1)
}

print('\n')

while (ge(i, 0)) {

  let pos = add(array, i)

  print(readByte(pos))

  i = sub(i,1)
}
