function main() {
  const ALPHABET_LENGTH = 'z' - 'a';
  var array = malloc(ALPHABET_LENGTH + 1);

  var i = 0;

  print(ALPHABET_LENGTH);

  while (i <= ALPHABET_LENGTH) {
    var pos = array + i;

    writeByte(pos, 'a' + i);

    print(readByte(pos));

    i = i + 1;
  }

  print('\n');

  while (i >= 0) {
    print(readByte(array + i));

    i = i - 1;
  }
}
