const MAX <- 1000

var i <- MAX / 10

var maxPalindrome <- 0

while (i < MAX) {
  var j <- MAX / 10

  while (j < MAX) {
    var val <- i * j


    if(val > maxPalindrome) {
      var test <- val
      var rev <- 0

      while(test > 0) {
        var digit <- test % 10
        rev <- rev * 10 + digit
        test <- test / 10
      }

      if( rev == val) {
        maxPalindrome <- val
      }
    }
    j <- j + 1
  }

  i <- i + 1
}

print("maxPalindrome: ")
print(maxPalindrome)