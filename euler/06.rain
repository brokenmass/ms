const MAX <- 100

var i <- 1

var squareSum <- 0
var sumSquare <- 0
while (i <= MAX) {
  squareSum <- squareSum + (i * i)
  sumSquare <- sumSquare + i

  i <- i + 1
}

sumSquare <- sumSquare * sumSquare


print("\nsquareSum: ")
print(squareSum)
print("\nsumSquare: ")
print(sumSquare)
print("\ndiff: ")
print(sumSquare - squareSum)