const COUNT <- 10001

const primes <- malloc(COUNT * 8)
var primesCount <- 1
writeQuad(primes, 2)

var i <- 3
while (primesCount < COUNT) {
  var index <- 0
  var continue <- 1

  while(continue) {

    if(index >= primesCount) {
      print("\nFound " )
      print(primesCount + 1)
      print(" prime: ")
      print(i)
      writeQuad(primes + (primesCount * 8), i)
      primesCount <- primesCount + 1
      continue <- 0
    } else if (i % readQuad(primes + (index * 8)) == 0) {
      continue <- 0
    }

    index <- index + 1
  }

  i <- i + 2
}
